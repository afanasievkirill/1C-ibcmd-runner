{
    "reportPath": "C:\\work\\", #Путь к файлам результатам тестов
    "reportFormat": "jUnit", #Формат отчета о пройденных тестах, доступные значения `jUnit', 'JSON'
    "showReport": true, # Открыть форму отчета по завершению тестирования.
    "closeAfterTests": true, #Параметр отвечающий за закрытие предприятия по завершению тестирования. Если хотим посмотреть результаты тестов, этот параметр устанавливаем в 'false'
    "filter": { # параметры фильтрации (откуда брать тесты для запуска)
        "extensions": [ #Для запуска всех тестов, реализованных в расширении с именем "YAXUNIT" указываем это расширение
            "YAXUNIT"
        ],
        # В случае, если достаточно запускать все тесты и они указаны в параметре 'extensions' нижеприведенные параметры не используются
        "modules": null, #Есть возможность указать конкретные модули в которых реализованы тесты
        "tests": null, #Список полных путей к исполняемым тестам ([ИмяМодуля.ИмяТеста])
        "contexts": null #Список имен контекстов, в которых будут запускаться тесты
    },
    "settings":{
        "ВТранзакции": true #Устанавливает настройку выполнения тестового метода в транзакции
    }
}
